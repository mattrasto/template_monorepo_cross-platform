{
  "name": "template_monorepo_cross_platform",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:client",

    "dev:client": "cross-env NODE_ENV=development npm-run-all --parallel dev:client:build dev:client:start",
    "dev:client:build": "cross-env NODE_ENV=development webpack --mode=development --config=webpack/webpack.config.js --env project=client",
    "dev:client:start": "cross-env NODE_ENV=development webpack serve --mode=development --config=webpack/webpack.config.js --env project=client",
    "dev:client-server": "cross-env NODE_ENV=development npm-run-all --sequential dev:client:build dev:client-server:start",
    "dev:client-server:start": "cross-env NODE_ENV=development node -r esm ./client/server.js",

    "---": "---",
    "staging": "cross-env NODE_ENV=staging npm-run-all --sequential staging:build staging:start",
    "staging:build": "cross-env NODE_ENV=staging npm run build",
    "staging:deploy": "node -r esm build/deploy.js staging --version=$(git rev-parse HEAD)",
    "staging:start": "cross-env NODE_ENV=staging node -r esm ./server.js",

    "prod": "cross-env NODE_ENV=production npm-run-all --sequential prod:build prod:start",
    "prod:build": "cross-env NODE_ENV=production npm run build",
    "prod:deploy": "node -r esm build/deploy.js production --version=$(git rev-parse HEAD)",
    "prod:start": "cross-env NODE_ENV=production node -r esm ./server.js",

    "build": "webpack --mode production --config webpack.config.js",
    "start": "npm run prod:start",

    "lint": "npm-run-all --sequential lint:js lint:styles:fix lint:vue:fix",
    "lint:js": "eslint src --ext .js",
    "lint:js:fix": "eslint src --ext .js --fix",
    "lint:styles": "stylelint src/**/*.vue",
    "lint:styles:fix": "stylelint src/**/*.vue --fix",
    "lint:vue": "eslint src --ext .vue",
    "lint:vue:fix": "eslint src --ext .vue --fix",
    "lint:fix": "eslint src --ext .js --fix",
    "prettify": "prettier \"src/**/*.{js,vue}\" --write"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "assign-deep": "^1.0.1",
    "auth0-js": "^9.19.0",
    "axios": "^0.27.2",
    "babel": "^6.23.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "~8.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "~10.0",
    "connect-history-api-fallback": "^2.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.23.3",
    "cross-env": "~7.0",
    "css-loader": "^6.7.1",
    "date-fns": "^2.28.0",
    "deepmerge": "^4.2.2",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "express-http-to-https": "^1.1.4",
    "html-webpack-plugin": "^5.5.0",
    "isbot": "^3.5.0",
    "millify": "^4.0.0",
    "mini-css-extract-plugin": "~2.6",
    "moment": "^2.29.3",
    "npm": "^8.13.2",
    "npm-run-all": "^4.1.5",
    "stylelint": "^14.9.1",
    "vue": "^3.2.37",
    "vue-loader": "^17.0.0",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "~4.0",
    "vue-style-loader": "^4.1.3",
    "vuex": "^4.0.2",
    "vuex-trace": "0.0.6",
    "webpack": "~5.73.0",
    "webpack-cli": "~4.10.0",
    "webpack-dev-server": "~4.9.3",
    "webpack-merge": "~5.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.6",
