{
  "name": "template_monorepo_cross_platform",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:client dev:api",
    "dev:api": "cross-env NODE_ENV=development npm-run-all --parallel dev:api:build dev:api:start",
    "dev:api:build": "cross-env NODE_ENV=development webpack --watch --mode=development --config=api/webpack/webpack.config.js",
    "dev:api:start": "nodemon --config api/nodemon.json -r esm api/dist/server.js",
    "dev:client": "npm-run-all --parallel dev:client:start",
    "dev:client:start": "vite --config client/vite/vite.config.js serve",
    "dev:client:build": "vite --config client/vite/vite.config.js build",
    "dev:client:preview": "vite --config client/vite/vite.config.js preview",
    "---": "---",
    "staging": "cross-env NODE_ENV=staging npm-run-all --sequential staging:build staging:start",
    "staging:build": "cross-env NODE_ENV=staging npm run build",
    "staging:deploy": "node -r esm build/deploy.js staging --version=$(git rev-parse HEAD)",
    "staging:start": "cross-env NODE_ENV=staging node -r esm ./server.js",
    "prod": "cross-env NODE_ENV=production npm-run-all --sequential prod:build prod:start",
    "prod:build": "cross-env NODE_ENV=production npm run build",
    "prod:deploy": "node -r esm build/deploy.js production --version=$(git rev-parse HEAD)",
    "prod:start": "cross-env NODE_ENV=production node -r esm ./server.js",
    "lint": "npm-run-all --sequential lint:js:fix lint:styles:fix lint:vue:fix",
    "lint:js": "eslint */src/* --ext .js",
    "lint:js:fix": "eslint */src/* --ext .js --fix",
    "lint:styles": "stylelint client/src/**/*.vue",
    "lint:styles:fix": "stylelint client/src/**/*.vue --fix",
    "lint:vue": "eslint client/src --ext .vue",
    "lint:vue:fix": "eslint client/src --ext .vue --fix"
  },
